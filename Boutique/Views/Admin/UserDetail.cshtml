@model IEnumerable<Visits>

@{
    Layout = "_AdminLayout";
}

<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Kullanici Giriş Detaylari
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>Giriş Tarihi</th>
                                        <th>Şehir</th>
                                        <th>IP-Adresi</th>
                                        <th>ISPN</th>
                                 
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="odd gradeX">
                                            <td>@item._Date</td>
                                            <td>@item.City</td>
                                            <td>@item.Query</td>
                                            <td>@item.Isp</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>